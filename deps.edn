           :test    {:extra-deps {lambdaisland/kaocha {:mvn/version "1.0.672"}}}
           :uberjar {:extra-deps {seancorfield/depstar {:mvn/version "1.1.104"}}
                     :main-opts  ["-m" "hf.depstar.uberjar" "fire-for-effect.jar"
                                  "-C" "-m" "hughpowell.co.uk.fire-for-effect"]}}}
