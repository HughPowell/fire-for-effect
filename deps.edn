{:paths   ["src" "resources"]
 :deps    {org.clojure/clojure    {:mvn/version "1.10.1"}
           duct/core              {:mvn/version "0.8.0"}
           duct/module.logging    {:mvn/version "0.5.0"}}
 :aliases {:dev     {:extra-paths ["dev/src" "dev/resources"]
                     :extra-deps  {integrant/repl {:mvn/version "0.3.1"}
